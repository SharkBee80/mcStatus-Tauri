{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "mcstatus",
  "version": "../package.json",
  "identifier": "com.sharkbee.mcstatus.tauri",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "echo npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "mcstatus",
        "width": 800,
        "height": 600,
        "minWidth": 400,
        "minHeight": 300,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}